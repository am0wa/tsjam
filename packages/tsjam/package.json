{
  "name": "tsjam",
  "version": "1.7.6",
  "description": "vanilla typescript sweets",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "node": {
        "import": "./lib/index.js",
        "types": "./lib/index.d.ts"
      },
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./money": {
      "node": {
        "import": "./lib/money/index.js",
        "types": "./lib/money/index.d.ts"
      },
      "import": "./lib/money/index.js",
      "types": "./lib/money/index.d.ts"
    },
    "./reactive": {
      "node": {
        "import": "./lib/reactive/index.js",
        "types": "./lib/reactive/index.d.ts"
      },
      "import": "./lib/reactive/index.js",
      "types": "./lib/reactive/index.d.ts"
    }
  },
  "sideEffects": false,
  "repository": "https://github.com/am0wa/tsjam",
  "license": "MIT",
  "keywords": [
    "typescript",
    "utils",
    "disposable",
    "rx-disposable",
    "rx-tools"
  ],
  "publishConfig": {
    "registry:": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc -b src/tsconfig.json",
    "build:watch": "tsc -b tsconfig.json -w",
    "lint": "eslint src",
    "test": "jest tests",
    "test:watch": "jest tests --watchAll",
    "doc": "run-s doc:html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --out docs",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d docs",
    "clean": "run-p clean:*",
    "clean:dist": "rimraf lib",
    "reset-hard": "git clean -f -d && git reset --hard && pnpm i",
    "patch-version": "pnpm version patch",
    "prepare-release": "run-s clean:dist lint test patch-version doc:html",
    "prepublishOnly": "run-s clean:dist build test",
    "publish-public": "pnpm publish --access=public",
    "postpublish": "git push --tags origin master"
  },
  "engines": {
    "node": ">=22"
  },
  "peerDependencies": {
    "rxjs": "^7.x"
  },
  "devDependencies": {
    "@tsjam/jest-config-recommended": "workspace:*",
    "@types/jest": "catalog:jest30",
    "gh-pages": "^3.2.3",
    "jest": "catalog:jest30",
    "npm-run-all": "catalog:",
    "rxjs": "^7.8.2",
    "ts-jest": "catalog:jest30",
    "typedoc": "^0.27.9",
    "typescript": "catalog:"
  },
  "files": [
    "lib",
    "tsconfig.json",
    "!**/tsconfig.tsbuildinfo"
  ],
  "prettier": "@tsjam/web-config-base/prettier.config.mjs"
}
